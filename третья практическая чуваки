#include <iostream>
using namespace std;
int main() {
	setlocale(0, "");
	int a;
	cout << "Введите число 0-100: ";
	cin >> a;

	if (a >= 1 && a <= 10) {
		cout << "Ваше число находится в диапазоне 1-10";
	}
	else if (a >= 11 && a <= 20) {
		cout << "Ваше число находится в диапазоне 11-20";
	}
	else if (a >= 21 && a <= 30) {
		cout << "Ваше число находится в диапазоне 21-30";
	}
	else if (a >= 31 && a <= 40) {
		cout << "Ваше число находится в диапазоне 31-40";
	}
	else if (a >= 41 && a <= 50) {
		cout << "Ваше число находится в диапазоне 41-50";
	}
	else if (a >= 51 && a <= 60) {
		cout << "Ваше число находится в диапазоне 51-60";
	}
	else if (a <= 61 && a >= 60) {
		cout << "Ваше число находится в диапазоне 61-70";
	}
	else if (a <= 71 && a >= 70) {
		cout << "Ваше число находится в диапазоне 71-80";
	}
	else if (a <= 81 && a >= 90) {
		cout << "Ваше число находится в диапазоне 81-90";
	}
	else if (a <= 91 && a >= 100) {
		cout << "Ваше число находится в диапазоне 91-100";
	}
	else {
		cout << "вы ввели число больше 100";
	}
}
